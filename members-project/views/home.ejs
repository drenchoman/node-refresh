<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Home</title>
    <meta name="description" content="" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1"
    />
    <link rel="stylesheet" href="" />
  </head>
  <body>
    <div>
  
        <div>
          <h1>Hacker Club</h1>
        </div>
        <div >
          <h2>Register or log in to make a post.</h2>
        </div>
          <div >
              <a href="/login">Login</a>
            
              <a href="/register">Register</a>
          </div>
      <div>
        <% messages.forEach(function(message) { %>
        <div>
          <h3><%= message.title %></h3>
          <p><%= message.message.replace(/[a-zA-Z]/g, 'x') %></p>
          <span><%= message.alias.replace(/[a-zA-Z]/g, 'x') %></span>
          <span><%= message.readable_date %></span>
          <img src="/images/<%= message.avatar_name %>.svg"
        </div>
      <% }) %>
      <img src="/images/home.svg" />
    </div>
    <!--[if lt IE 7]>
      <p class="browsehappy">
        You are using an <strong>outdated</strong> browser. Please
        <a href="#">upgrade your browser</a> to improve your
        experience.
      </p>
    <![endif]-->

    <script src="" async defer></script>
  </body>
</html>
